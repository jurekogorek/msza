      <%if miejsce.kraj%>
        <%= link_to miejsce.kraj.try(:nazwa), miejsce.kraj %><br />
      <%end%>
      <%= miejsce.adres %>
    </p>
    <h5>Terminy mszy:</h5>
    <%miejsce.terminy.each do |termin|%>
      <%= termin.dzien.nazwa %>:<%= termin.godzina %> <%=  termin.komentarz %>
    <%end%>
  </div>
  <%=link_to "PokaÅ¼",miejsce%>
</div>